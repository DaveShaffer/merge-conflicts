<h3>New Satellite!</h3>
<fieldset>
  <form method="POST" action="/sats">
    <div class="form-group col-md-4">
      <input name="noradNum" class="form-control" placeholder="NORAD Number"/>
    </div>
    <div class="form-group col-md-4">
      <input name="name"  class="form-control" placeholder="Satellite Name"/>
    </div>
    <div class="form-group col-md-4">
      <input name="country"  class="form-control" placeholder="Launched By"/>
    </div>
    <div class="form-group col-md-4">
      <input name="launchDate" class="form-control" placeholder="Date Launched"/>
    </div>
    <div class="form-group col-md-4">
      <input name="purpose"  class="form-control" placeholder="Purpose"/>
    </div>
    <div class="form-group col-md-4">
      <input name="perigee"  class="form-control" placeholder="Perigee Km"/>
    </div>
    <div class="form-group col-md-4">
      <input name="apogee" class="form-control" placeholder="Apogee Km"/>
    </div>
    <div class="form-group col-md-4">
      <input class="btn btn-primary col-md-12" type="submit" value="Submit">
    </div>
  </form>
</fieldset>
